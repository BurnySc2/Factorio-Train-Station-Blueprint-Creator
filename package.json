{
	"name": "factorio-train-station-blueprint-creator",
	"version": "0.1.0",
	"private": true,
	"homepage": "./",
	"dependencies": {
		"@biomejs/biome": "^2.3.8",
		"@testing-library/jest-dom": "^6.1.4",
		"@testing-library/react": "^16.1.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.8",
		"@types/node": "^20.9.0",
		"@types/react": "^18.2.37",
		"@types/react-dom": "^18.2.15",
		"@types/react-router-dom": "^5.3.3",
		"clone-deep": "^4.0.1",
		"copy-to-clipboard": "^3.3.3",
		"lodash.isequal": "^4.5.0",
		"lodash.pick": "^4.4.0",
		"pako": "^2.1.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "^6.20.1",
		"react-scripts": "^5.0.1",
		"react-tooltip": "^5.23.0",
		"tailwindcss": "^3.0.0",
		"typescript": "^4.9.5"
	},
	"scripts": {
		"start": "npm run tailwind:dev && BROWSER=none NODE_OPTIONS='--openssl-legacy-provider' react-scripts start",
		"build": "npm run tailwind:prod && react-scripts build",
		"tailwind:dev": "DEVELOPMENT=true tailwind build -i src/tailwind.css -o src/index.css",
		"tailwind:prod": "tailwind build -i src/tailwind.css -c tailwind.config.js -o src/index.css",
		"lint-format": "biome check --write",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"watch": {
		"build": {
			"patterns": [
				"src"
			],
			"extensions": "ts,tsx,json"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/pako": "^2.0.4"
	}
}
